# Database
DATABASE_URL="postgresql://USER:PASSWORD@HOST:PORT/DB?schema=public"

# SerpApi (Google engine)
SERPAPI_API_KEY=""
# Optional - if not set, SerpApi will infer location.
SERPAPI_LOCATION=""
# Optional - comma-separated locations to rotate (geo routing). Example:
# SERPAPI_LOCATIONS=United States,Canada,United Kingdom,Australia
SERPAPI_LOCATIONS=""
SERPAPI_GL="us"
SERPAPI_HL="en"
SERPAPI_NUM="10"

# Search provider routing
# SEARCH_PROVIDER=serpapi|apify_bing|bing
# SEARCH_FALLBACK=apify_bing|serpapi|bing
SEARCH_PROVIDER="serpapi"
SEARCH_FALLBACK=""

# Apify (optional) - used when SEARCH_PROVIDER or SEARCH_FALLBACK is apify_bing
APIFY_TOKEN=""

# Groq (optional) - used for LLM classification/extraction when enabled
GROQ_API_KEY=""
GROQ_MODEL_FAST="llama-3.1-8b-instant"
GROQ_MODEL_SMART="llama-3.3-70b-versatile"

# Defaults
SEKED_MARKET="en-US"
SEKED_FRESHNESS="Week"
SEKED_MAX_RESULTS_PER_QUERY="10"

# Safety / rate limits
SEKED_MAX_RUN_QUERIES="5"
SEKED_MIN_SCORE_QUALIFIED="70"

# Attribution
ORDER_PAGE_URL="https://example.com/order"
UTM_SOURCE_DEFAULT="seked"
UTM_MEDIUM_DEFAULT="dm"
UTM_CAMPAIGN_DEFAULT="lead-operator"
